        <activity android:name=".Dogglers" />
        <activity android:name=".RecyclerView" />
        <activity android:name=".TipCounterActivity" />
        <activity android:name=".DiceJava" />
        <activity android:name=".DiceClass" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>